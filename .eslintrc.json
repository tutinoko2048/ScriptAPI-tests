{
    "extends": ["eslint:recommended"],
    "plugins": ["jsdoc"],
    "env": {
      "node": true,
      "es2021": true
    },
    "globals": {},
    "rules": {
      "no-unused-vars": "warn",
      "no-empty": "off",
      "block-spacing": "warn",
      "arrow-spacing": "warn",
      "space-before-blocks": "warn",
      "keyword-spacing": "warn",
      "no-irregular-whitespace": "warn",
      "jsdoc/no-undefined-types": 1
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
}